<Window x:Class="practicum_5.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:practicum_5"
        mc:Ignorable="d"
        Title="Register" Height="450" Width="800">
    <Grid>
        <Button Content="Registreer" HorizontalAlignment="Left" Margin="391,198,0,0" VerticalAlignment="Top" Width="102" Click="Register_Button_Click" Height="30"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="254,104,0,0" VerticalAlignment="Top"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="254,239,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.642,2.904"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="254,135,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="239" Name="UsernameField" />
        <TextBox HorizontalAlignment="Left" Height="23" Margin="254,270,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="239" Name="password" />

        <TextBlock HorizontalAlignment="Left" Margin="304,341,0,0" TextWrapping="Wrap" VerticalAlignment="Top">
            Already got a account? <Hyperlink Click="To_Login">Click here!</Hyperlink>
        </TextBlock>
    </Grid>
</Window>
